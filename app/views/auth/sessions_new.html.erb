<h1>ログイン</h1>
<%= form_with(url: session_login_path, local: true) do |f| %>
  <div>
    <%= f.label :email %>
    <%= f.email_field :email %>
  </div>

  <div>
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>

  <%= f.submit 'ログイン' %>
<% end %>

<%= link_to '新規登録', user_new_path %>